<html lang="en" class="app">
<head>
	<meta charset="utf-8" />
	<title>Notebook | Web Application</title>
	<meta name="description" content="app, web app, responsive, admin dashboard, admin, flat, flat ui, ui kit, off screen nav" />
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
	<link rel="stylesheet" href="css/app.v2.css" type="text/css" />
	<script type="text/javascript" src="js/jquery-1.9.1.min.js"></script>
	<script type="text/javascript" src="js/master.js"></script>
</head>
<body>
<!-- 通讯录start -->
<section class="hbox stretch"> 
	<!-- 左版块start --> 
	<aside>
		<!-- 个人邮箱详细显示start --> 
		<section class="vbox"> 
			<!-- 操作标题start -->
			<header class="header bg-white b-b clearfix">
			<div class="row m-t-sm">
				<div class="col-sm-8 m-b-xs">
					<div class="btn-group">
						<button type="button" class="btn btn-sm btn-default" title="写邮件" id="mail_show" style="display:none">
							<i class="fa fa-envelope-o"></i>
						</button>
						<button type="button" class="btn btn-sm btn-default" title="移除此联系人" id="show" style="display:none">
							<i class="fa fa-trash-o"></i>
						</button>
						<button type="button" class="btn btn-sm btn-default" title="添加到联系人" id="show1" style="display:none">
							<i class="fa fa-plus"></i>
						</button>
					</div>
					<!-- <a href="modal.html" data-toggle="ajaxModal"
						class="btn btn-sm btn-default"><i class="fa fa-plus"></i></a> -->
				</div>
				<div class="col-sm-4 m-b-xs">
					<%--搜索框 --%>
					<div class="input-group">
						<input type="text" class="input-sm form-control" id="contact_query"
							placeholder="(搜索联系人姓名)"> 
						<span class="input-group-btn">
							<button class="btn btn-sm btn-default" id="emps_query" type="button">查找</button>
						</span>
						<a href="#subNav" data-toggle="class:hide" class="btn btn-sm btn-default active" title="展开/收起">
							<i class="fa fa-caret-left text-active fa-lg"></i>
							<i class="fa fa-caret-right text fa-lg"></i>
						</a>
					</div>				
				</div>
			</div>
			</header> 
			<!-- 操作标题end --> 
		
			<!-- 显示具体内容start--> 
			<section class="scrollable wrapper w-f"> 
				<section class="panel panel-default">
					<div class="table-responsive" id="emp_table_show">
						<table class="table table-striped m-b-none" id="tableSort">
							<caption></caption>
							<thead>
							</thead>
							<tbody>
							</tbody>
						</table>
						<div class="login_no" style="display: none;"></div>
					</div>
				</section> 
			</section> 
			<!-- 显示具体内容end --> 
		
			<!-- 分页页码显示start --> 
			<!-- <footer class="footer bg-white b-t">
				<div class="row text-center-xs">
					<div class="col-md-6 hidden-sm">
						<p class="text-muted m-t">Showing 20-30 of 50</p>
					</div>
					<div class="col-md-6 col-sm-12 text-right text-center-xs">
						<ul class="pagination pagination-sm m-t-sm m-b-none">
							<li><a href="#"><i class="fa fa-chevron-left"></i></a></li>
							<li class="active"><a href="#">1</a></li>
							<li><a href="#">2</a></li>
							<li><a href="#">3</a></li>
							<li><a href="#">4</a></li>
							<li><a href="#">5</a></li>
							<li><a href="#"><i class="fa fa-chevron-right"></i></a></li>
						</ul>
					</div>
				</div>
			</footer>  -->
			<!-- 分页页码显示end --> 
		</section> 
		<!-- 个人邮箱详细显示end --> 
	</aside> 
	<!-- 左版块end --> 
	
	<!-- 右版块start -->
	<aside class="aside-md bg-white b-r" id="subNav">
		<div class="wrapper b-b header contact_title">通讯录 <span id="user_notice"></span></div>
		<div class="wrapper b-b header contact_title">我的部门</div>
		<%--显示我的部门 --%>
		<div class="master_dept_mine dept_title"></div>
		<div class="wrapper b-b header contact_title">其他部门</div>
		<%--循环显示其他部门 --%>
		<div class="master_dept_other dept_title"></div>
		<%--联系人 --%>
		<div class="wrapper b-b header contact_title">联系人</div>
		<%--常用联系人--%>
		<div class="master_contact_mine"></div>
	</aside> 
	<!-- 右版块end --> 
</section>
<!-- 通讯录end -->

<script src="js/app.v2.js"></script> <!-- Bootstrap --> <!-- App -->

<!-- 个人资料弹框start -->
<div class="modal fade" id="modal">
	<div class="modal-dialog">
			<div class="modal-content emp_infos">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"
						aria-hidden="true">&times;</button>
					<h4 class="modal-title">个人资料</h4>
				</div>
				<div class="modal-body">
					<section class="panel panel-default m-l-n-md m-r-n-md m-b-none">
					<header class="panel-heading"> <span
						class="label bg-danger pull-right">部门</span><span id="emp_dept"></span></header>
					<table class="table table-striped m-b-none text-sm">
						<thead>
							<tr>
								<th>职位</th>
								<th></th>
								<th></th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>员工号</td>
								<td></td>
								<td rowspan="6"></td>
							</tr>
							<tr>
								<td>姓名</td>
								<td></td>
							</tr>
							<tr>
								<td>性别</td>
								<td></td>
							</tr>
							<tr>
								<td>邮箱</td>
								<td></td>
							</tr>
							<tr>
								<td>电话</td>
								<td></td>
							</tr>
							<tr>
								<td>住址</td>
								<td></td>
							</tr>
							<!-- <tr>
								<td>来往记录</td>
								<td></td>
							</tr> -->
						</tbody>
					</table>
					</section>
				</div>
			</div>
		</div>
</div>
<!-- 个人资料弹框end -->
